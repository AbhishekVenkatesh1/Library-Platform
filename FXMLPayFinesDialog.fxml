<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane id="AnchorPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9" fx:controller="checkingout.FXMLPayFinesDialogController">
   <children>
      <HBox layoutX="120.0" layoutY="179.0" prefHeight="28.0" prefWidth="369.0">
         <children>
            <Label prefHeight="27.0" prefWidth="82.0" text="Fine Amount:" />
            <TextField fx:id="fineAmount">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#clickPayFine" prefHeight="27.0" prefWidth="80.0" text="Pay Fine">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox layoutX="331.0" layoutY="224.0" prefHeight="27.0" prefWidth="158.0">
         <children>
            <Button mnemonicParsing="false" onAction="#cancel" prefHeight="27.0" prefWidth="80.0" text="Cancel">
               <HBox.margin>
                  <Insets left="80.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label fx:id="success" layoutX="126.0" layoutY="269.0" prefHeight="36.0" prefWidth="410.0" />
      <Label fx:id="errorMessage" layoutX="126.0" layoutY="338.0" prefHeight="36.0" prefWidth="410.0" />
   </children>
</AnchorPane>
