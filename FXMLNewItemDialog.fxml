<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="administrative.FXMLNewItemDialogController">
   <children>
      <HBox layoutX="14.0" layoutY="175.0" prefHeight="30.0" prefWidth="573.0">
         <children>
            <Label text="Item Type:" />
            <TextField fx:id="itemType" prefHeight="27.0" prefWidth="85.0">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </TextField>
            <Label text="Title:" />
            <TextField fx:id="title" prefHeight="27.0" prefWidth="94.0">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </TextField>
            <Label text="Value:" />
            <TextField fx:id="value" prefHeight="27.0" prefWidth="71.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#newItem" prefHeight="27.0" prefWidth="100.0" text="Create Item">
               <HBox.margin>
                  <Insets left="25.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox layoutX="429.0" layoutY="219.0" prefHeight="27.0" prefWidth="152.0">
         <children>
            <Button mnemonicParsing="false" onAction="#cancel" prefHeight="27.0" prefWidth="100.0" text="Cancel">
               <HBox.margin>
                  <Insets left="45.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label fx:id="printItemId" layoutX="191.0" layoutY="247.0" prefHeight="47.0" prefWidth="219.0" />
      <Label fx:id="errorMessage" layoutX="192.0" layoutY="318.0" prefHeight="46.0" prefWidth="215.0" />
   </children>
</AnchorPane>
