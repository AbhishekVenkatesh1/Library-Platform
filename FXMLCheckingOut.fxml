            <Label prefHeight="26.0" prefWidth="64.0" text="Patron ID:">
               <HBox.margin>
                  <Insets top="30.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="patronId" prefHeight="27.0" prefWidth="124.0">
               <HBox.margin>
                  <Insets left="5.0" top="30.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#clickEnter" prefHeight="27.0" prefWidth="77.0" text="Enter">
               <HBox.margin>
                  <Insets left="30.0" top="30.0" />
