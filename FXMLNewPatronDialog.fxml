<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="administrative.FXMLNewPatronDialogController">
   <children>
      <HBox layoutX="45.0" layoutY="177.0" prefHeight="47.0" prefWidth="510.0">
         <children>
            <Label prefHeight="17.0" prefWidth="40.0" text="Name:" />
            <TextField fx:id="patronName" prefHeight="27.0" prefWidth="103.0">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </TextField>
            <Label prefHeight="17.0" prefWidth="39.0" text="Email:" />
            <TextField fx:id="patronEmail" prefHeight="27.0" prefWidth="149.0">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#newPatron" prefHeight="27.0" prefWidth="110.0" text="Create Patron">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox layoutX="367.0" layoutY="224.0" prefHeight="62.0" prefWidth="192.0">
         <children>
            <Button mnemonicParsing="false" onAction="#cancel" prefHeight="27.0" prefWidth="110.0" text="Cancel">
               <HBox.margin>
                  <Insets left="70.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label fx:id="printPatronId" layoutX="176.0" layoutY="260.0" prefHeight="39.0" prefWidth="248.0" />
      <Label fx:id="errorMessage" layoutX="176.0" layoutY="310.0" prefHeight="39.0" prefWidth="248.0" />
   </children>
</AnchorPane>
