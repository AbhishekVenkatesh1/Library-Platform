<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="checkingin.FXMLCheckingInController">
   <children>
      <HBox layoutX="71.0" layoutY="176.0" prefHeight="30.0" prefWidth="331.0">
         <children>
            <Label prefHeight="29.0" prefWidth="49.0" text="Item Id:" />
            <TextField fx:id="itemId">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#clickCheckIn" text="Check In">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label fx:id="success" layoutX="84.0" layoutY="266.0" prefHeight="41.0" prefWidth="343.0" textAlignment="CENTER" />
      <Label fx:id="errorMessage" layoutX="84.0" layoutY="320.0" prefHeight="49.0" prefWidth="343.0" />
   </children>
</AnchorPane>
